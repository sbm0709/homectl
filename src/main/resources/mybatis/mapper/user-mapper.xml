<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.home.homectl.mapper.UserMapper">

    <select id="find_user" resultType="UserDTO">
        SELECT * FROM user
        WHERE id = #{id}
    </select>

    <insert id="user_register">
        INSERT user (`id`, `pw`)
        VALUES
        (#{id}, #{pw})
    </insert>

</mapper>